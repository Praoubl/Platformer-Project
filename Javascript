var reload=function(a){
  blocks = [];
  extraBlocks=[];
  if(isNaN(a)){
    items=[{
      t:["5"],
      x:[200],
      y:[200],
      s:[20]
    },{
      t:["shield"],
      x:[200],
      y:[300],
      s:[20]
    },{
      t:[""],
      x:[200],
      y:[200],
      s:[20]
    },{
      t:[""],
      x:[200],
      y:[200],
      s:[20]
    },{
      t:[""],
      x:[200],
      y:[200],
      s:[20]
    }];
    gravityBlocks[height]=[];
    levelEntities=[{npc1:{x:0},fly1:{x:0}},{npc1:{x:0},fly1:{x:0}},{npc1:{x:0},fly1:{x:0}},{npc1:{x:0},fly1:{x:0}}];
  }
  for (var y = 0;y < level.length; y++) {
    for (var x = 0;x < level[y].length; x++) {
      isBlock[y][x]=false;
      isHazard[y][x]=false;
      if (level[y][x]==="1") { 
        blocks.push(new block(x*20,y*20,0)); 
        isBlock[y][x]=true;
      }else if(level[y][x]==="2"){
        blocks.push(new block(x*20,y*20,1));
        isBlock[y][x]=true;
      }else if(level[y][x]==="l"){
        blocks.push(new lava(x*20,y*20,0));
        isBlock[y][x]=true;
        isHazard[y][x]=true;
      }else if(level[y][x]==="g"){
        blocks.push(new goal(x*20,y*20,0));
        isBlock[y][x]=true;
      }else if(level[y][x]==="3"){
        blocks.push(new block(x*20,y*20,2));
        isBlock[y][x]=true;
      }else if(level[y][x]==="4"){
        blocks.push(new block(x*20,y*20,3));
        isBlock[y][x]=true;
      }else if(level[y][x]==="-"){
        blocks.push(new wall(x*20,y*20,0));
      }else if(level[y][x]==="_"){
        blocks.push(new wall(x*20,y*20,1));
      }else if(level[y][x]===","){
        blocks.push(new wall(x*20,y*20,2));
      }else if(level[y][x]==="."){
        blocks.push(new wall(x*20,y*20,3));
      }else if(level[y][x]==="t"){
        blocks.push(new trampoline(x*20,y*20,0));
        isBlock[y][x]=true;
      }else if(level[y][x]==="s"){
        blocks.push(new supply(x*20,y*20,0));
      }else if(level[y][x]==="c"){
        blocks.push(new wall(x*20,y*20,2));
        if(isNaN(a)){
          gravityBlocks[height].push(new crate(x*20,y*20,0));
        }
      }else if(level[y][x]==="C"){
        blocks.push(new wall(x*20,y*20,3));
        if(isNaN(a)){
          gravityBlocks[height].push(new crate(x*20,y*20,0));
        }
      }else if(level[y][x]==="A"){
        blocks.push(new wall(x*20,y*20,2));
        extraBlocks.push(new spike(x*20,y*20,0));
        isHazard[y][x]=true;
      }else if(level[y][x]==="V"){
        blocks.push(new wall(x*20,y*20,3));
        extraBlocks.push(new spike(x*20,y*20,0));
        isHazard[y][x]=true;
      }else{
        isBlock[y][x]=false;
      }
    }
  }
  npc1.acceleration=0;
  fly1.xeleration=0;
  fly1.acceleration=0;
  //blocks.push(npc1);
  //blocks.push(npc2);
  //blocks.push(npc3);
  //blocks.push(npc4);
  midb=true;
  kc=0;
  //levelFunctions[0];
  if(isNaN(a)){
    player1.x=levelStartCoords.x;
    player1.y=levelStartCoords.y;
    player1.acceleration = 0;
    player1.hp=20;
    player1.ammo=10;
    npc1.hp=20;
    npc1.move=true;
    npc1.opinion=1;
    fly1.move=true;
    fly1.hp=10;
  }
  levelFunctions[height](a);
  //if(!isNan(items[height].x[0])){
    for(i=0;i<items[height].t.length;i++){
      if(items[height].t[i]!==""){
        blocks.push(new item(items[height].x[i],items[height].y[i],items[height].t[i],true,items[height].s[i],blocks.length+1,i));
      }
    }
  //}
}
