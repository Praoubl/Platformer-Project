    ["---", "..-", "..-", ".-.", "-.."],//7
    [".-.", "-.-", ".-.", "-.-", ".-."],//8
    [".-.", "-.-", ".--", "..-", "--."],//9
    ["."]
];
var possibleLetters = 'ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz.,\'!;)?-_0123456789 '.split('');
var myTextSize = 3;
var txtSize = function(sz){
    myTextSize = sz;
};
var letter = function(which, x, y){
    var lett = [];
    for(i = 0; i < possibleLetters.length; i ++){
        if(which === possibleLetters[i]){
            lett = letterCodes[i];
        }
    }
    
    //noStroke();
    for(i = 0; i < lett.length; i ++){
        for(j = 0; j < lett[i].length; j ++){
            if(lett[i][j] === '-'){
                ctx.fillRect((j*myTextSize) + x, (i*myTextSize) + y-myTextSize*5, myTextSize, myTextSize);
                ctx.fillRect((j*myTextSize) + x, (i*myTextSize) + y-myTextSize*5, myTextSize, myTextSize);
            }
        }
    }
};
  var l = 0;
var txt = function(txteq, x, y){
    for(l = 0; l < txteq.length; l ++){
        letter(txteq[l], x + (l*(myTextSize*3.5)), y);
    }
};}//pixel font
