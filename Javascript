    ctx.fillStyle='rgb(175, 133, 40)';
    ctx.fillRect(this.x, this.y, 20, 10);
    ctx.fillStyle='rgb(185, 144, 60)';
    ctx.fillRect(this.x, this.y+10, 10, 10);
