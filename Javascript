draw = function() { 
  //ctx.translate(player1.x,player1.y);
  var canvas = document.getElementById('canvas');
  var ctx = canvas.getContext('2d');
  ctx.clearRect(0,0,canvas.width,canvas.height);
  ctx.transform(2,0,0,2,canvas.width/2-player1.x*2,canvas.height/2-player1.y*2);
  if(height!==levelNum2){
    levelEntities[levelNum2].npc1.x=npc1.x;
    levelEntities[levelNum2].npc1.y=npc1.y;
    levelEntities[levelNum2].npc1.hp=npc1.hp;
    levelEntities[levelNum2].npc1.opinion=npc1.opinion;
    levelEntities[levelNum2].fly1.x=fly1.x;
    levelEntities[levelNum2].fly1.y=fly1.y;
    levelEntities[levelNum2].fly1.hp=fly1.hp;
    level=levels[height];
    //levelStartCoords.x=startCoords[height].x;
    //levelStartCoords.y=startCoords[height].y;
    levelNum2=height;
    reload(1);
    if(loaded[height]){
    }else{
      loaded[height]=true;
      loaded[height+1]=false;
    }
    if(!isNaN(levelEntities[height].npc1.y)){
      //bannerText("qwertyuiop",player1.x,player1.y);
      npc1.x=levelEntities[height].npc1.x;
      npc1.y=levelEntities[height].npc1.y;
      npc1.hp=levelEntities[height].npc1.hp;
      npc1.opinion=levelEntities[height].npc1.opinion;
      fly1.x=levelEntities[height].fly1.x;
      fly1.y=levelEntities[height].fly1.y;
      fly1.hp=levelEntities[height].fly1.hp;
    }
  }
  if(height===0){
    ctx.clearRect(0,0,canvas.width,canvas.height);
  }else{
    ctx.clearRect(0,0,canvas.width,canvas.height);
  }
  for (var i = 0;i < blocks.length; i++) {
    blocks[i].display();
  }
  for (var i = 0;i < extraBlocks.length; i++) {
    extraBlocks[i].display();
  }
  for (var i = 0;i < gravityBlocks[height].length; i++) {
    gravityBlocks[height][i].display();//yjod yslrd qasvr sgyrt yjr zryrptd gtpz s drddopm om yjr drbrmyj shr
  }
  ctx.font = '10px sans-serif';//11b+11b+11b+3b=19.61
  ctx.fillText(input.restart,2,10);//3b+3b+3b+11b=19.99
  ctx.fillText(player1.hp,2,20);//small fall+small fall+small fall+run=
  ctx.fillText(player1.ammo,2,80);
  ctx.fillText(npc1.opinion,2,30);
  ctx.fillText(kc,2,40);
  ctx.fillText(mouseX,2,50);
  ctx.fillText(mouseY,2,60);
  ctx.fillText(height,2,70);
  if(height===0){
    if(npc1.x<61){
      n1="right"
    }else if(npc1.x>520){
      n1="left";
    }
    if(npc1.x===240&&n1==="right"||npc1.x===340&&n1==="left"){
      /*npc1.acceleration = -5.4;
      npc1.jumping=true;*/
      npc1.jump=true;
    }else{
      npc1.jump=false;
      //bannerText("hOI!",npc1.x,npc1.y);
    }
    //bannerText(npc1.acceleration+"",player1.x,player1.y);
    if(n1==="right"){
      npc1.left=false;
      npc1.right=true;
    }else if(n1==="left"){
      npc1.right=false;
      npc1.left=true;//hop:3.63 jump:10.15 1 bounce:10.13 2:5.04 3:5.03 run:6
//fall:6.43 4:5.04 5:5.03 6:5.04 7:5.03 8:9.91 9:9.89 10:4.91 11:4.9 12:4.91
//13:4.9 14:4.91 15:4.9 16:4.65 small fall:2.41 small bounce:3.53 
    }//hop+jump+run=19.78 (order matters)
  }//2b+2b+8b=19.99
  npc1.display();
  fly1.display();
  player1.display();
  interact=false;
  if(input.z){
    ctx.fillStyle='rgb(255,0,0)';
    ctx.fillRect(player1.x,player1.y,20,20);
  }
  talkT--;
  if(talkT<=0){
    talkNum2=talkNum;
    talkT=25;
  }else{
    talkT--;
  }
  ctx.setTransform(1, 0, 0, 1, 0, 0);
  midNums[0]=Math.floor((canvas.width-20)/(3*80));
  midNums[1]=Math.floor((canvas.height-20)/(3*80));
  if(player1.inventory===true&&mouseX>canvas.width/6+10&&mouseX<canvas.width/6+80*(midNums[0])+10&&mouseY>canvas.height/6+10&&mouseY<canvas.height/6+10+2*80*midNums[1]){//main inventory
    boxRect(canvas.width/6+20,canvas.height/6-1,midNums[0]*80-20,15);
    ctx.fillStyle= 'rgb(250, 250, 250)';
    ctx.font = '11px sans-serif';
    ctx.fillText(player1.items[Math.floor(((mouseX-canvas.width/6)-10)/80)+midNums[0]*Math.floor(((mouseY-canvas.height/6-10)/80))],canvas.width/6+24,canvas.height/6+10);
    if(mouseIsClicked){
      midNums[2]=player1.inventoryPick;
      player1.inventoryPick=player1.items[Math.floor(((mouseX-canvas.width/6)-10)/80)+midNums[0]*Math.floor((mouseY-canvas.height/6-10)/80)];
      //ctx.fillStyle='rgb(250,50,50)';
      //ctx.fillRect(0,0,100,100);
      player1.items[Math.floor(((mouseX-canvas.width/6)-10)/80)+midNums[0]*Math.floor((mouseY-canvas.height/6-10)/80)]=midNums[2];
    }
  }else if(player1.inventory&&!(mouseX>canvas.width/6+10)||player1.inventory&&!(mouseX<5*canvas.width/6)||player1.inventory&&!(mouseY>canvas.height/6-7)||player1.inventory&&!(mouseY<canvas.height/6+20+2*80*midNums[1])){//outside inventory
    //ctx.fillRect(0,0,100,100);
    if(mouseIsClicked){
      if(player1.inventoryPick!==""){
        items[height].x.push(player1.x);
        items[height].y.push(player1.y);
        items[height].t.push(player1.inventoryPick);
        items[height].s.push(20);
        player1.inventoryPick="";
      }
    }
  }else if(player1.inventory&&mouseX>canvas.width/2+10&&mouseY>canvas.height/6+10&&mouseX<canvas.width/2+90&&mouseY<canvas.height/6+90){
    boxRect(canvas.width/6+20,canvas.height/6-1,midNums[0]*80-20,15);
    ctx.fillStyle= 'rgb(250, 250, 250)';
    ctx.font = '11px sans-serif';
    ctx.fillText(player1.weopan,canvas.width/6+24,canvas.height/6+10);
    if(mouseIsClicked){
      midNums[2]=player1.inventoryPick;
      player1.inventoryPick=player1.weopan;
      //ctx.fillStyle='rgb(250,50,50)';
      //ctx.fillRect(0,0,100,100);
      player1.weopan=midNums[2];
    }
  }
  if(player1.inventory&&player1.inventoryPick.length>0){
    boxRect(mouseX-3,mouseY-13,player1.inventoryPick.length*7+4,14,2);
    ctx.fillStyle= 'rgb(250, 250, 250)';
    ctx.font = '11px sans-serif';
    ctx.fillText(player1.inventoryPick,mouseX,mouseY-2);
  }
  ctx.setTransform(2,0,0,2,canvas.width/2-player1.x*2,canvas.height/2-player1.y*2);
  /*midNums[0]=Math.floor((canvas.width-20)/(3*80));
  midNums[1]=Math.floor((canvas.height-20)/(3*80));
  ctx.fillStyle='rgba(50,50,50,0.3)';
  ctx.fillRect(0,0,canvas.width,canvas.height);
  boxRect(canvas.width/6,canvas.height/6,2*canvas.width/3,20+2*80*midNums[1]);
  ctx.fillStyle= 'rgb(0, 0, 0)';
  ctx.font = '15px sans-serif';
  ctx.fillText("press z to exit",canvas.width-93,12);
  for(i=0;i<player1.capacity;i++){
    boxRect(canvas.width/6+80*(i%midNums[0])+20,canvas.height/6+80*Math.floor(i/midNums[0])+20,60,60);
  }*/
  //bannerText(npc1.acceleration+"",player1.x,player1.y-20);
  //reload();
  //height=0;
  if(input.x){
    //ctx.setTransform(1, 0, 0, 1, 0, 0);
    //ctx.fillRect(100,100,100,100);
  }
  talkT2++;
  timer++;
  //ctx.translate(-player1.x,-player1.y);
  ctx.setTransform(1, 0, 0, 1, 0, 0);
  mouseIsClicked=false;
  window.requestAnimationFrame(draw);
};

window.requestAnimationFrame(draw);
